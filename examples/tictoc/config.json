{
  // Global parameters
  "global" : {

    // Simulation time limit
    "timeLimit" : 100,

    // Time scale exponent
    //   0 = seconds
    //  -3 = ms
    //  -6 = us
    //  -9 = ns
    // -12 = ps
    "timeScaleExp" : -3.0

    // Log simulation messages to this file
    //"logFileName" : "sim.out"
  },

  // Domain configuration
  "domain" : {

    // Domain name
    "name" : "SimpleDomain",

    // Initialization file
    "initFileName" : "init.json",

    // Domain modules
    "modules" : {
      "tic" : "Tictoc",
      "toc" : "Tictoc"
    }
  },

  // Web server configuration
  "webserver" : {

    "port" : 8080,

    "threads" : 10,

    "documentRoot" : "./www",

    // Connection timeout (in seconds)
    "timeout" : 1
  }
}